{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "dotnet",
			"task": "build",
			"problemMatcher": [
				"$msCompile"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "dotnet: build"
		},
		{
			"label": "dotnet: publish",
			"type": "process",
			"command": "dotnet",
			"args": [
				"publish",
				"${workspaceFolder}/GithubIo.csproj",
				"-c",
				"Release",
				"-o",
				"${workspaceFolder}/../publish"
			],
		}
	]
}